{
    "extends": [
      "development"
    ],
    "hints": {
      "no-inline-styles": "off",
      "meta-viewport": "off",
      "disown-opener": "off",
      "axe/text-alternatives": [
        "default",
        {
          "image-alt": "off"
        }
      ],
      "axe/name-role-value": [
        "default",
        {
          "link-name": "off"
        }
      ],
      "compat-api/css": [
        "default",
        {
          "ignore": [
            "overflow-wrap: anywhere"
          ]
        }
      ],
      "axe/aria": [
        "default",
        {
          "aria-progressbar-name": "off"
        }
      ]
    }
  }